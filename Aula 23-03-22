a = int(input ("Insira o valor a"))
b = int(input ("Insira o valor b"))
c = int(input ("Insira o valor c"))

if b < c or b < a:
    print ("O valor de B é menor do que o A, ou menor do que o C")
else:
    print("O valor de B é maior do que o A, ou é menor do que o C")